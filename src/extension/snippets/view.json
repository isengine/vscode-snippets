{
	"isEngine view get instance": {
		"scope": "php",
		"prefix": ".v",
		"body": [
			"\\$view = View::getInstance();"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine view blocks": {
		"scope": "php",
		"prefix": ".vb",
		"body": [
			"\\$view -> get('block') -> launch('$1');"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine view content": {
		"scope": "php",
		"prefix": ".vc",
		"body": [
			"\\$view -> get('content') -> getData($1);"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine view display": {
		"scope": "php",
		"prefix": ".vd",
		"body": [
			"\\$view -> get('display') -> addBuffer($1);"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine view get": {
		"scope": "php",
		"prefix": ".vg",
		"body": [
			"\\$view -> get('$1');"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine view items": {
		"scope": "php",
		"prefix": ".vi",
		"body": [
			"\\$view -> get('block') -> launch('items:$1', 'default');"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine view lang": {
		"scope": "php",
		"prefix": ".vl",
		"body": [
			"\\$view -> get('lang|$1');"
		],
		"description": "isEngine snippets group..."
