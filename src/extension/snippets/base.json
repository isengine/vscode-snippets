		],
		"description": "isEngine snippets group..."
	},
	"isEngine base view template": {
		"scope": "php",
		"prefix": "111",
		"body": [
			"<?php",
			"",
			"namespace is;",
			"",
			"use is\\Helpers\\System;",
			"use is\\Helpers\\Strings;",
			"use is\\Helpers\\Objects;",
			"use is\\Masters\\View;",
			"",
			"\\$view = View::getInstance();",
			"",
			"$1",
			"",
			"?>"
		],
		"description": "isEngine snippets group..."
	},
	"isEngine foreach operator": {
		"scope": "php",
		"prefix": "2",
		"body": [
			"foreach (\\$$1 as \\$key$2 => \\$item$3) {",
			"	$4",
			"}",
			"unset(\\$key$5, \\$item$6);"
		],
		"description": "isEngine snippets group..."
	},
